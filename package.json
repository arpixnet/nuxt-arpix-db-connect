{
  "name": "nuxt-arpix-db-connect",
  "version": "2.0.0",
  "description": "Generic GraphQL client module for Nuxt 3/4 with graphql-request and graphql-ws",
  "repository": {
    "type": "git",
    "url": "https://github.com/arpixnet/nuxt-arpix-db-connect"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.20.2",
    "graphql": "^16.12.0",
    "graphql-request": "^7.4.0",
    "graphql-ws": "^6.0.6",
    "jwt-decode": "^4.0.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.7.0",
    "@nuxt/eslint-config": "^1.12.1",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^3.20.2",
    "@nuxt/test-utils": "^3.21.0",
    "@types/node": "latest",
    "changelogen": "^0.6.2",
    "eslint": "^9.39.2",
    "nuxt": "^3.20.2",
    "typescript": "~5.9.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.12"
  }
}
